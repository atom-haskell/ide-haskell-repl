{
  "name": "ide-haskell-repl",
  "main": "./lib/ide-haskell-repl",
  "version": "0.6.0",
  "description": "Haskell REPL support for ide-haskell",
  "keywords": [
    "ide-haskell",
    "ide",
    "haskell",
    "repl"
  ],
  "activationHooks": [
    "language-haskell:grammar-used"
  ],
  "repository": "https://github.com/atom-haskell/ide-haskell-repl",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "dependencies": {
    "atom-haskell-utils": "^0.9.0",
    "atom-highlight": "^0.3.0",
    "etch": "^0.9.5",
    "fuzzaldrin": "^2.1.0",
    "promise-queue": "^2.2.3",
    "sub-atom": "^1.0.0",
    "tree-kill": "^1.0.0",
    "underscore-plus": "^1.6.6"
  },
  "consumedServices": {
    "ide-haskell-upi": {
      "description": "Uses ide-haskell's unified pluggable interface",
      "versions": {
        "^0.3.0": "consumeUPI"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "3.0.0": "autocompleteProvider_3_0_0"
      }
    }
  },
  "deserializers": {
    "IdeHaskellReplView": "createReplView"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1"
  }
}
