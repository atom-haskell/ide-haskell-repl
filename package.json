{
  "name": "ide-haskell-repl",
  "main": "./lib/ide-haskell-repl",
  "version": "0.7.0",
  "description": "Haskell REPL support for ide-haskell",
  "keywords": [
    "ide-haskell",
    "ide",
    "haskell",
    "repl"
  ],
  "activationHooks": [
    "language-haskell:grammar-used"
  ],
  "repository": "https://github.com/atom-haskell/ide-haskell-repl",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "dependencies": {
    "atom-haskell-utils": "^1.0.0",
    "atom-highlight": "^0.3.0",
    "etch": "^0.12.5",
    "fuzzaldrin": "^2.1.0",
    "promise-queue": "^2.2.3",
    "tree-kill": "^1.0.0"
  },
  "consumedServices": {
    "ide-haskell-upi": {
      "description": "Uses ide-haskell's unified pluggable interface",
      "versions": {
        "^0.3.0": "consumeUPI"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "3.0.0": "autocompleteProvider_3_0_0"
      }
    }
  },
  "deserializers": {
    "IdeHaskellReplView": "createReplView"
  },
  "devDependencies": {
    "@types/fuzzaldrin": "^2.1.0",
    "@types/node": "^8.0.14",
    "atom-haskell-tslint-rules": "0.0.6",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
